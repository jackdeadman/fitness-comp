<% provide(:title) do
  "Add Participant"
end %>

<%= @challenge.title %>

<form method="post" action=<%="#{create_participant_path(@challenge)}"%>>
<%= select('form', :user_id, User.all.collect { |v| [ v.username, v.id ] }) %>
<%= hidden_field_tag :authenticity_token, form_authenticity_token %>
<input class="btn btn-primary" type="submit", value="Add person" />
</form>
